{
  "manifest_version": 2,
  "name": "Taringa Tools",
  "description": "Community Tools for improve experience and content",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.taringa.net/*"],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ]
}
